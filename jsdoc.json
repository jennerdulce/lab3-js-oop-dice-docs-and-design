{
  "source": {
    "include": [
      "./src",
      "./README.md"
    ],
    "includePattern": "\\.(js)$",
    "excludePattern": "node_modules/"
  },
  "opts": {
    "destination": "./docs/generated/",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
